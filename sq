Índices:
    "arc_documentos_pkey" PRIMARY KEY, btree (id)
    "ix_arc_documentos_arc_juzgado_origen_id" btree (arc_juzgado_origen_id)
    "ix_arc_documentos_autoridad_id" btree (autoridad_id)
