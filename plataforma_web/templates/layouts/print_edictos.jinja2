{% extends 'layouts/base.jinja2' %}

{% block template_head %}
    <link href="{{ url_for('static', filename='css/bs5-print.css') }}" rel="stylesheet" />
{% endblock %}

{% block main %}
    <header id="header">
        <div class="row">
            <div class="col">
                <img src="https://storage.googleapis.com/pjecz-informatica/static/images/pjecz-horizontal.png" alt="PJECZ">
            </div>
        </div>
    </header>
    <article id="body">
        {% block content %}{% endblock %}
    </article>
{% endblock %}

{% block template_javascript %}{% endblock %}
