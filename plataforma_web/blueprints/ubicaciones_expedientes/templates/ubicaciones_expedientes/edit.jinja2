{% extends 'layouts/app.jinja2' %}
{% import 'macros/form.jinja2' as f with context %}
{% import 'macros/topbar.jinja2' as topbar %}

{% block title %}Editar Ubicaci√≥n de Expediente{% endblock %}

{% block topbar_actions %}
    {{ topbar.page('Editar ' + ubicacion_expediente.expediente + ' del ' + ubicacion_expediente.autoridad.clave) }}
{% endblock %}

{% block content %}
    {% call f.card() %}
        {% set form_kwargs = {'ubicacion_expediente_id': ubicacion_expediente.id} %}
        {% call f.form_tag('ubicaciones_expedientes.edit', fid='ubicacion_expediente_form', **form_kwargs) %}
            {% call f.form_group(form.expediente) %}{% endcall %}
            {% call f.form_group(form.ubicacion) %}{% endcall %}
            {% call f.form_group(form.guardar) %}{% endcall %}
        {% endcall %}
    {% endcall %}
{% endblock %}
