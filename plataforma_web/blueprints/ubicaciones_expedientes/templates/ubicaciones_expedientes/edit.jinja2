{% extends 'layouts/app.jinja2' %}
{% import 'macros/form.jinja2' as f with context %}

{% block title %}Editar Ubicacion de Expediente{% endblock %}

{% block content %}
    {{ f.page_breadcrumb('Editar ' + ubicaciones_expedientes.expediente) }}
        <div class="container-fluid">
        {% set form_kwargs = {'ubicaciones_expedientes_id': ubicaciones_expedientes.id} %}
        {% call f.form_tag('ubicaciones_expedientes.edit', fid='ubicaciones_expedientes_form', **form_kwargs) %}
            {% call f.form_group(form.expediente) %}{% endcall %}
            {% call f.form_group(form.ubicacion) %}{% endcall %}
            {{ form.guardar() }}
        {% endcall %}
    </div>
{% endblock %}
